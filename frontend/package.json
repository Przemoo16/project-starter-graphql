{
  "name": "frontend",
  "description": "Frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/node-server/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "generate-graphql-types": "graphql-codegen --config codegen.ts",
    "lint": "yarn run prettier && yarn run eslint && yarn run tsc",
    "prettier": "prettier --write .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.node-server",
    "test": "vitest run",
    "tsc": "tsc",
    "qwik": "qwik",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US --assetsPath=i18n"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.3",
    "@builder.io/qwik-city": "^1.4.3",
    "@graphql-codegen/cli": "5.0.1",
    "@graphql-codegen/typescript": "4.0.6",
    "@modular-forms/qwik": "^0.23.1",
    "@types/eslint": "^8.56.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-qwik": "^1.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "postcss": "8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "qwik-speak": "^0.21.0",
    "tailwindcss": "3.4.1",
    "typescript": "^5.3.3",
    "undici": "^6.6.1",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
