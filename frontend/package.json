{
  "name": "frontend",
  "description": "Frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/node-server/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "generate-graphql-types": "graphql-codegen --config codegen.ts",
    "lint": "yarn run tsc && yarn run eslint",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.node-server",
    "test": "vitest run",
    "tsc": "tsc",
    "qwik": "qwik",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US --assetsPath=i18n"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.3.0",
    "@builder.io/qwik-city": "^1.3.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@modular-forms/qwik": "^0.21.2",
    "@types/eslint": "^8.44.8",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-qwik": "^1.3.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "postcss": "8.4.32",
    "qwik-speak": "^0.18.0",
    "tailwindcss": "3.3.6",
    "typescript": "^5.3.2",
    "undici": "^5.28.2",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.0.1"
  }
}
